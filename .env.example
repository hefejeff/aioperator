# Gemini AI API Key (required)
API_KEY=your_gemini_api_key_here

# OpenAI API Key (optional - for custom assistants/agents)
# Note: Must use VITE_ prefix to be accessible in the browser
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Firebase Configuration (required)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your_project.firebaseio.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Gamma AI API Key (optional - for presentation generation)
VITE_GAMMA_API_KEY=your_gamma_api_key_here
